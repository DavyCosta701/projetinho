<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="wasabi.sushi.app.projeto.banco.ClienteEntity">
        <table name="cliente" schema="formacaojava" catalog=""/>
        <attributes>
            <id name="idCliente">
                <column name="idCliente"/>
            </id>
            <basic name="nome">
                <column name="nome"/>
            </basic>
            <basic name="cpf">
                <column name="cpf"/>
            </basic>
            <basic name="email">
                <column name="email"/>
            </basic>
        </attributes>
    </entity>
    <entity class="wasabi.sushi.app.projeto.banco.CursoEntity">
        <table name="curso" schema="formacaojava" catalog=""/>
        <attributes>
            <id name="idCurso">
                <column name="idCurso"/>
            </id>
            <basic name="nome">
                <column name="nome"/>
            </basic>
            <basic name="valor">
                <column name="valor"/>
            </basic>
            <basic name="url">
                <column name="url"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
